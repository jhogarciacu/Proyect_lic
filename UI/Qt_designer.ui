  <property name="styleSheet">
   <string notr="true">/* 1. FONDO DE LA VENTANA: Gris metálico oscuro */
QMainWindow {
    background-color: #31363b;
}

/* 2. CABECERA: El degradado azul profundo con borde neón */
QFrame#frame_title { 
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #1a2a44, stop:1 #0d1522);
    border: 1px solid #000000;
    border-bottom: 3px solid #00c3ff; /* El azul neón de la imagen */
}

/* Título de la cabecera */
QLabel#label_titulo_principal {
    color: #ffffff;
    font-size: 20pt;
    font-weight: bold;
    font-family: &quot;Arial Black&quot;, Gadget, sans-serif;
    letter-spacing: 2px;
}

/* 3. LOS CUADROS (GROUPBOX): Gris acero con borde doble */
QGroupBox {
    background-color: #eff0f1; /* Fondo gris claro interno */
    border: 2px solid #76797c;
    border-radius: 4px;
    margin-top: 1.5em; /* Espacio para el título */
    font-weight: bold;
    color: #31363b;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 3px 0 3px;
}

/* 4. EL FRAME INTERNO (Donde va el texto &quot;Ángulo de giro&quot;) */
/* En tu imagen, este cuadro tiene un fondo gris oscuro y bordes redondeados */
QGroupBox QFrame {
    background-color: #bdc3c7; /* Gris plateado */
    border: 1px solid #76797c;
    border-radius: 2px;
}

/* 5. LABELS INTERNOS (Ángulo de giro, etc) */
QGroupBox QLabel {
    color: #232629;
    font-weight: bold;
    background: transparent;
    border: none;
}

/* 6. BOTONES: El acabado metálico con relieve */
QPushButton {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #fcfcfc, stop:1 #bdc3c7);
    border: 1px solid #76797c;
    border-radius: 3px;
    color: #232629;
    padding: 5px 15px;
    min-height: 25px;
}

QPushButton:hover {
    background-color: #dbe0e2;
}

QPushButton:pressed {
    background-color: #95a5a6;
    padding-left: 6px;
    padding-top: 6px;
}

/* Botones Especiales: INICIAR y DETENER */
QPushButton#btn_iniciar {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #5dbd5d, stop:1 #2d6b2d);
    color: white;
}

QPushButton#btn_detener {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #e74c3c, stop:1 #962d22);
    color: white;
}

/* 7. LA TABLA: Fondo blanco, líneas grises */
QTableWidget {
    background-color: #ffffff;
    alternate-background-color: #f7f7f7;
    gridline-color: #bdc3c7;
    border: 1px solid #76797c;
    color: #232629;
}

QHeaderView::section {
    background-color: #d1d5d8;
    padding: 2px;
    border: 1px solid #76797c;
    font-weight: bold;
}</string>
